<% include header %>
<div class="container">
  <div class="container-fluid">
    <div class="row-fluid">
      <div class="span9">
        <h3><%= resources.translate("Topics(formal)")%></h3>
        <table class="table table-bordered table-striped">
          <thead>
            <tr>
              <th><%= resources.translate("Topic")%></th>
              <th><%= resources.translate("Speakers")%></th>
            </tr>
          </thead>
          <tbody>
            <% topics.forEach(function (topic) { %>
            <tr data-id="<%=topic._id%>">
              <td><%=topic.name%></td>
              <td><%=topic.speaker%></td>
            </tr>
            <% }); %>
          </tbody>
        </table>
      </div>
      <div class="span3">
        <% include sidebar %>
      </div>
    </div>
  </div>
</div>
<% include footer %>